// Hello, World! Running String /Anatoliy Besplemennov (STARTOS)

char txt [5]={"Hello"};	

void Main(void)
{   int i;

    for( i = 136; i > 8; --i )
		{
                 //	X	 Y		 String
     Print_String( i,  150, "Hello, World!");
     Print_String(  i, 170, txt);
 			
     Delay(20);	// Delay 20 mS
    }

    Exit();     // Exit to OS
}
